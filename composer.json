{
    "name": "archerzdip/laravel-identity",
    "description": "Chinese ID number generate and verity.",
    "license": "MIT",
    "keywords":
    [
        "chinese id number",
        "laravel",
        "identity",
        "verity",
        "identity card"
    ],
    "authors": [
        {
            "name": "archerZdip",
            "email": "646121218@qq.com"
        }
    ],
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "ArcherZdip\\Identity\\": "src"
        }
    },
    "require": {
        "php": ">=7.1",
        "illuminate/support": ">=5.0.0",
        "illuminate/console": ">=5.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArcherZdip\\Identity\\IdentityServiceProvider"
            ],
            "aliases": {
                "Identity": "ArcherZdip\\Identity\\IdentityFacade"
            }
        }
    }
}
